{
  "name": "sweetalert-plus-plus",
  "version": "1.0.0",
  "type": "module",
  "description": "Enterprise-grade, accessible, customizable modal and alert library with 30+ form field types, theme system, and advanced visual effects",
  "main": "dist/sweetalert-plus-plus.cjs.js",
  "module": "dist/sweetalert-plus-plus.esm.js",
  "browser": "dist/sweetalert-plus-plus.umd.js",
  "unpkg": "dist/sweetalert-plus-plus.min.js",
  "jsdelivr": "dist/sweetalert-plus-plus.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sweetalert-plus-plus.esm.js",
      "require": "./dist/sweetalert-plus-plus.cjs.js",
      "browser": "./dist/sweetalert-plus-plus.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core.esm.js",
      "require": "./dist/core.cjs.js",
      "types": "./dist/core.d.ts"
    },
    "./toast": {
      "import": "./dist/toast.esm.js",
      "require": "./dist/toast.cjs.js",
      "types": "./dist/toast.d.ts"
    },
    "./theme": {
      "import": "./dist/theme.esm.js",
      "require": "./dist/theme.cjs.js",
      "types": "./dist/theme.d.ts"
    },
    "./css": "./dist/sweetalert-plus-plus.css",
    "./css/minimal": "./dist/sweetalert-plus-plus.minimal.css",
    "./css/dark": "./dist/sweetalert-plus-plus.dark.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "rollup -c rollup.config.js",
    "build:css": "node scripts/build-css.cjs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:cdn": "npm run build && npm run build:minify",
    "build:minify": "terser dist/sweetalert-plus-plus.umd.js -o dist/sweetalert-plus-plus.min.js -c -m",
    "dev": "rollup -c -w",
    "serve": "npx serve .",
    "test": "vitest",
    "test:a11y": "vitest --config vitest.a11y.config.js",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "size": "size-limit",
    "prepublishOnly": "npm run build:cdn"
  },
  "keywords": [
    "modal",
    "dialog",
    "alert",
    "confirm",
    "prompt",
    "toast",
    "notification",
    "popup",
    "accessible",
    "a11y",
    "lightweight",
    "form",
    "form-builder",
    "theme",
    "dark-mode",
    "neumorphism",
    "glassmorphism",
    "enterprise",
    "sweetalert",
    "swal"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaiAnk/Sweetalertplusplus.git"
  },
  "bugs": {
    "url": "https://github.com/RaiAnk/Sweetalertplusplus/issues"
  },
  "homepage": "https://raiank.github.io/sweetalertplusplus.github.io/",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^6.0.0",
    "postcss": "^8.4.0",
    "rollup": "^4.0.0",
    "terser": "^5.24.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15"
  },
  "size-limit": [
    {
      "path": "dist/core.esm.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/sweetalert-plus-plus.esm.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/sweetalert-plus-plus.min.js",
      "limit": "20 KB"
    }
  ],
  "dependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.53.3"
  }
}
